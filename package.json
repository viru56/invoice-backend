{
  "name": "indi-invoice",
  "version": "1.0.0",
  "description": "api server for indi-invoice",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch '*.ts' --exec ts-node ./server/server.ts",
    "start": "node ./build/server.js",
    "start_with_cluster": "START_WITH_CLUSTER=true  ts-node ./server/server.ts",
    "prod": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://viru56@bitbucket.org/teaminvoiceindia/invoice-server.git"
  },
  "keywords": [
    "node",
    "typesctipt"
  ],
  "author": "virender nehra",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/teaminvoiceindia/invoice-server/issues"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "braintree": "^3.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.12",
    "pdfkit": "^0.14.0",
    "ts-node": "^10.9.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.25",
    "nodemon": "^3.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.4.2"
  }
}
