{
  "name": "indi-invoice",
  "version": "1.0.0",
  "description": "api server for indi-invoice",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch '*.ts' --exec ts-node ./server/server.ts",
    "start": "node ./build/server.js",
    "start_with_cluster": "START_WITH_CLUSTER=true  ts-node ./server/server.ts",
    "prod": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://viru56@bitbucket.org/teaminvoiceindia/invoice-server.git"
  },
  "keywords": [
    "node",
    "typesctipt"
  ],
  "author": "virender nehra",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/teaminvoiceindia/invoice-server/issues"
  },
  "dependencies": {
    "@types/node": "^12.6.8",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "mongoose": "^6.12.7",
    "multer": "^1.4.2",
    "nodemailer": "^5.1.1",
    "pdfkit": "^0.10.0",
    "ts-node": "^8.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^8.3.0",
    "nodemon": "^1.19.1",
    "tslint": "^5.18.0",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=12.0.0 <14.19.1"
  }
}
